<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
     
  <Description about="urn:mozilla:install-manifest">
    <em:id>kwikemarks@jacob.stair</em:id>
    <em:version>0.2</em:version>
    <em:type>2</em:type>
    <em:optionsURL>chrome://kwikemarks/content/options.xul</em:optionsURL>
    <em:optionsType>2</em:optionsType>
    
    <!-- Target Application this extension can install into, with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>36.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Front End MetaData -->
    <em:name>Kwik-E-Marks</em:name>
    <em:description>A nice interface to quickly view the bookmarks in your bookmarks menu.</em:description>
    <em:creator>Jacob Stair</em:creator>
    <em:homepageURL>https://github.com/bobtheowl/Kwik-E-Marks</em:homepageURL>
    <em:iconURL>chrome://kwikemarks/skin/kwikemarks-icon-48x48.png</em:iconURL>
    <em:icon64URL>chrome://kwikemarks/skin/kwikemarks-icon-64x64.png</em:icon64URL>
  </Description>
</RDF>